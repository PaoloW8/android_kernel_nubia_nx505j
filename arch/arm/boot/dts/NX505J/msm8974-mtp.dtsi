	                vdd-supply = <&pm8941_l18>;
                    vio-supply = <&pm8941_lvs1>;
	        };

	        maxq616_i2c_adapter@11 {
	                status = "okay";
	                reg = <0x11>;
	                compatible = "uei,maxq616";
					vdd-chip-supply = <&pm8941_l23>;
